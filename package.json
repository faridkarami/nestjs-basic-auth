{
  "name": "nestjs-basic-auth",
  "version": "0.0.0",
  "description": "HTTP basic auth middleware for NestJS",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "basic",
    "auth"
  ],
  "author": "Farid Karami",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faridkarami/nestjs-basic-auth.git"
  },
  "homepage": "https://github.com/faridkarami/nestjs-basic-auth#readme",
  "bugs": {
    "url": "https://github.com/faridkarami/nestjs-basic-auth/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
